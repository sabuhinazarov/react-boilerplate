{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate with webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "",
    "dev": "NODE_ENV=development npm run clean && npm run webpack:dev",
    "clean": "rimraf build",
    "webpack:prod": "webpack --mode=production",
    "webpack:dev": "webpack --mode=development --watch",
    "build": "NODE_ENV=production npm run clean && npm run webpack:prod"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "typescript",
    "sass"
  ],
  "author": "Sabuhi Nazarov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "sass": "^1.48.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.4.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@sabuhinazarov/node-common": "^1.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
